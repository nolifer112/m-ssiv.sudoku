def matrix3(): # 3x3 listide list, ühes punktis 1-9
    matrix3 = []
    for i in range(0,3): # rea loendur, kolm rida
        row = []
        for j in range(0,3): # ruudu loendur, kolm ruutu
            square = []
            for x in range(1,10): # ruut, 1-9 arvud
                square.append(x)
            row.append(square)
        matrix3.append(row)
    return matrix3

